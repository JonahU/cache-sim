    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Daxpy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "daxpy", "-d", "100", "-n", "1"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Small cache",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "daxpy", "-d", "100", "-n", "1", "-c", "512"]
        }
    ]
}