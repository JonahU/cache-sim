    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Daxpy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "daxpy", "-d", "100", "-n", "1", "-p"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Random Small cache daxpy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "daxpy", "-d", "100", "-n", "4", "-c", "512", "-r", "random"]
        },
        {
            "type": "node",
            "request": "launch",
