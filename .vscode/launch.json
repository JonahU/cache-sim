    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Daxpy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "daxpy", "-d", "100", "-n", "1"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Random Small cache daxpy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "daxpy", "-d", "100", "-n", "4", "-c", "512", "-r", "random"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "FIFO Small cache daxpy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "daxpy", "-d", "100", "-n", "4", "-c", "512", "-r", "FIFO"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Random large cache daxpy",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "daxpy", "-d", "100", "-n", "4", "-r", "random"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Simple mxm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-a", "mxm", "-d", "3", "-n", "4", "-r", "random", "-p"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Simple mxm_block",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/cache-sim.js",
            "args": ["-d", "3", "-n", "4", "-r", "random", "-f", "3"]
        }
    ]
}